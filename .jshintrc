{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "quotmark": "double",
  "regexp": true,
  "undef": true,
  "unused": true,
  "noempty": true,
  "strict": true,
  "trailing": true,
  "smarttabs": false,

  "predef": [
    "define",
    "require",
    "$",
    "tableauSoftware"
    "_",
    "expect",
    "it",
    "describe",
    "beforeEach"
  ]
}
